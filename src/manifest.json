{
  "manifest_version": 3,
  "name": "Online bookmarks",
  "version": "1.0",
  "description": "Extension to chrome's bookmarks.",
  "icons": {
    "16": "images/icon-16.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "side_panel": {
    "default_path": "sidepanel.html"
  },
  "permissions": ["sidePanel", "bookmarks"],
  "action": {
    "default_title": "popup.html"
  },
  "background": {
    "service_worker": "service_worker.js"
  }
}